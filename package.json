{
  "name": "feathers-fletching",
  "version": "0.0.1",
  "description": "Hooks, services, and plugins for feathers.js",
  "main": "src/",
  "repository": "https://github.com/DaddyWarbucks/feathers-fletching.git",
  "author": "DaddyWarbucks <techtbeau@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "sh deploy-docs.sh",
    "lint": "eslint src/.",
    "fixcode": "eslint src/. --fix"
  },
  "dependencies": {
    "@feathersjs/errors": "^4.3.4"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "vuepress": "^1.2.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "trailingComma": "none",
          "singleQuote": true,
          "printWidth": 80
        }
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true
    }
  }
}
