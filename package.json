{
  "name": "feathers-fletching",
  "version": "1.20.5",
  "description": "Hooks, services, and plugins for feathers.js",
  "main": "src/",
  "repository": "https://github.com/DaddyWarbucks/feathers-fletching.git",
  "author": "DaddyWarbucks <techtbeau@gmail.com>",
  "homepage": "https://daddywarbucks.github.io/feathers-fletching/",
  "license": "MIT",
  "keywords": [
    "feathers",
    "feathersjs",
    "hooks"
  ],
  "private": false,
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "sh deploy-docs.sh",
    "build": "unbuild",
    "lint": "eslint src/.",
    "fixcode": "eslint src/. --fix",
    "test": "npm run lint && vitest run",
    "test:run": "vitest run",
    "vitest": "vitest",
    "release:patch": "npm run test && npm version patch && npm publish",
    "release:minor": "npm run test && npm version minor && npm publish",
    "release:major": "npm run test && npm version major && npm publish"
  },
  "dependencies": {
    "@feathersjs/errors": "^5.0.6",
    "lru-cache": "^10.0.0",
    "qs": "^6.11.2",
    "unset-value": "^2.0.1"
  },
  "devDependencies": {
    "@feathersjs/feathers": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "feathers-memory": "^4.1.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "rate-limiter-flexible": "^2.4.1",
    "unbuild": "^1.2.1",
    "vitest": "^0.32.2",
    "vuepress": "^1.9.9"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "trailingComma": "none",
          "singleQuote": true,
          "printWidth": 80
        }
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "es6": true
    }
  }
}
